// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;

int search(int arr[], int x, int n){
    for (int i=0; i < n; i++){
        if (arr [i]==x){
            return i;
        }
    }
    return -1;
}

int main() {
    // Write C++ code here
    int arr[]={20,30,10,100,40};
    int x=100;
    cout<<search(arr, x, 5);
}
